{
  "name": "web proxy control",
  "version": "0.1",
  "manifest_version": 2,
  "description": "代理配置程序 支持HTTPS/SOCKS/HTTP代理，支持域名排除名单",
  "icons": { "128": "icon_128.png" },
  "background": {
    "persistent": false,
    "scripts": ["bg.js"]
  },
  "permissions": ["proxy","tabs"],
  "browser_action": {
    "default_title": "",
    "default_icon": "icon_19.png",
    "default_popup": "popup.html"
  }
}